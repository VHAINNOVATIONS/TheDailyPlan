/* --------------------------------------------------------------- */
/* ---------------------- DARK BLUE BUTTON ----------------------- */


#home_universe .clear,
#universe .clear { /* generic container (i.e. div) for floating buttons */
    overflow: hidden;
    width: 100%;
}

#home_universe .button,
#universe .button {
    background: transparent url('../images/login_button.gif') no-repeat scroll top right;
    color: #FFF;
    display: block;
    float: left;
    font: normal 12px arial, sans-serif;
    height: 24px;
    margin-right: 6px;
    padding-right: 18px; /* sliding doors padding */
    text-decoration: none;
}

#home_universe input.button,
#universe input.button {
    background: transparent url('../images/login_button.gif') no-repeat scroll top right;
    color: #FFF;
    display: block;
    float: left;
    font: normal 12px arial, sans-serif;
    height: 24px;
    margin-left: 6px;
	padding: 0 9px 3px;
}

#home_universe a.button span,
#home_universe input.button span,
#universe a.button span,
#universe input.button span {
    background: transparent url('../images/login_button.gif') no-repeat;
    display: block;
    line-height: 14px;
    padding: 5px 0 5px 18px;
} 

#home_universe a.button:active,
#home_universe input.button:active,
#universe a.button:active,
#universe input.button:active {
    background-position: bottom right;
    color: #8FB1CB;
    outline: none; /* hide dotted outline in Firefox */
}

#home_universe a.button:active span,
#home_universe input.button:active span,
#universe a.button:active span,
#universe input.button:active span {
    background-position: bottom left;
    padding: 6px 0 4px 18px; /* push text down 1px */
} 




/* --------------------------------------------------------------- */
/* ---------------------- DARK GRAY BUTTON ----------------------- */


#universe .clear { /* generic container (i.e. div) for floating buttons */
    overflow: hidden;
    width: 100%;
}

#universe a.graybutton {
    background: transparent url('../images/gray_button.gif') no-repeat scroll top right;
    color: #FFF;
    display: block;
    float: right;
    font: normal 12px arial, sans-serif;
    height: 22px;
    padding-right: 18px; /* sliding doors padding */
    padding-left:21px;
	padding-top:5px;
    text-decoration: none;
}

#home_universe a.graybutton span {
    background: transparent url('../images/gray_button.gif') no-repeat;
    display: block;
    line-height: 12px;
    padding: 5px 0 5px 18px;
} 

#home_universe a.graybutton:active {
    background-position: bottom right;
    color: #DDD;
    outline: none; /* hide dotted outline in Firefox */
}

#home_universe a.graybutton:active span {
    background-position: bottom left;
    padding: 6px 0 4px 18px; /* push text down 1px */
} 







		
	


/* --------------------------------------------------------------- */
/* ------------------------ SEARCH TAB --------------------------- */


#universe .clear { /* generic container (i.e. div) for floating buttons */
    overflow: hidden;
    width: 100%;
}

#universe a.tab {
    background: transparent url('../images/tab.gif') no-repeat scroll top right;
    color: #AAA;
    display: block;
    float: left;
    font: bold 22px arial, sans-serif;
    height: 45px;
	width:72px;
    margin-right: 6px;
    padding-right: 18px; /* sliding doors padding */
    text-decoration: none;
}

#universe a.tab span {
    background: transparent url('../images/tab.gif') no-repeat;
    display: block;
    line-height: 25px;
    padding: 10px 0 10px 18px;
} 
	
#universe a.tab.selected {
	background-color: #ffffff;
	background: none;
}

#universe a.tab.selected span {
	background: none;
	color:#005CA3;
	margin:0px;
}

#universe a.tab:active, #universe a.tab:hover {
    background-position: bottom right;
	color:#999;
    outline: none; /* hide dotted outline in Firefox */
}

#universe a.tab:active span, #universe a.tab:hover span {
    background-position: bottom left;
    padding: 11px 0 9px 18px; /* push text down 1px */
} 

#universe div.tab_content {
	display: none;
}
	
	
/* --------------------------------------------------------------- */
/* ----------------------- TOOLTIP POP-UP ------------------------ */

	

#universe span.expandable {
	text-decoration: underline;
	color:#005CA3;
}

#tooltip {
	position: absolute;
	z-index: 3000;
	border: 1px solid #111;
	background-color: #eee;
	padding: 5px;
	opacity: 0.85;
}
#tooltip h3, #tooltip div { margin: 0; }

#tooltip.pretty {
	background:url("images/shadow.png") repeat scroll 0 0 transparent;
	border:medium none;
	font-family:Arial;
	height:135px;
	opacity:0.8;
	padding:20px;
	width:210px;
}
#tooltip.pretty h3 {
	font-size:12pt;
	margin-bottom:0.75em;
	text-align:center;
	width:220px;
}
#tooltip.pretty div {
	text-align:left;
	width:220px;
}
#tooltip.fancy {
	background:url("images/shadow2.png") repeat scroll 0 0 transparent;
	height:100px;
	padding-top:5em;
}
#tooltip.fancy.viewport-bottom {
	background:url("images/shadow3.png") repeat scroll 0 0 transparent;
	height:100px;
	padding-top:1em;
	padding-bottom:4em;
}