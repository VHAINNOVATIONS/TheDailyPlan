/* --------------------------------------------------------------- */
/* -------------------- BASIC TAG ALTERATIONS -------------------- */
	html {
		margin: 0;
		padding: 0;
	}
	body {
		font-family: Arial, Helvetica, sans-serif;
		background-color:#FFF;
		margin: 0px;
		padding: 0px;
	}

	#home_universe table,
	#universe table { 
		width: 100%;
	}

	#home_universe td,
	#universe td { 
		margin: 0px;
		padding: .1em; 
		vertical-align: top;
		line-height: 1.2em;
	}

	#home_universe thead,
	#universe thead { 
		line-height:1.1em;
		vertical-align:bottom;
	}
	
	#home_universe ol, #home_universe ul,
	#universe ol, #universe ul {
		list-style-type: none;
		font-size:12px;
		line-height:16px;
		color:#000;
		margin: 0px 0px 10px 0px;
		padding: 0px 5px 0px 0px;
	}
	
	#home_universe ol.bullets, #home_universe ul.bullets,
	#universe ol.bullets, #universe ul.bullets {
		list-style-type: circle;
		font-size:12px;
		line-height:0px;
		color:#000;
		margin: 0 0 0 25px;
		padding: 0;
	}
	#home_universe li,
	#universe li {
		font-weight: normal;
		margin: 0;
		padding: 10px;
	}
	
	#home_universe h1,
	#universe h1 {
		font-family:Arial, Helvetica, sans-serif;
		font-weight:bold;
		font-size: 22px;
	}
	#home_universe h2,
	#universe h2 {
		font-size: 16px;
		font-weight:bold;
	}
	
	#home_universe h3,
	#universe h3 {
		font-size: 15px;
		font-weight: bold;
	}
	#home_universe h4,
	#universe h4 {
		font-size: 14px;
		font-weight: bold;
		padding:0px;
		margin:0px;
	}
	#home_universe h5 {
		font-size: 14px;
		margin: 7px 0px 7px 0px;
		font-weight: normal;
	}
	#home_universe h6 {
		font-family:Arial, Helvetica, sans-serif;
		font-size:13px;
		color:#003468;
		margin:5px 0 0 0;
		font-weight:normal;
	}
	#home_universe p,#home_universe label,
	#universe p, #universe label {
		font-family:Arial, Helvetica, sans-serif;
		font-weight: normal;
		color:#333333;
		margin:0px;
		line-height:1em;
	}
	
	#home_universe a,
	#universe a  {
		color:#0056A0;
		text-decoration:underline;
	}
	
	#home_universe a:hover,
	#universe a:hover {
		color:#0099FF;
		text-decoration:none;
	}
	
	#home_universe blockquote,
	#universe blockquote {
		margin: 0;
		padding: 0 0 0 20px;
	}
	#home_universe form,
	#universe form  {
		font-family:Arial, Helvetica, sans-serif;
		margin: 0;
		padding: 0;
		vertical-align:middle;
	}
	#home_universe input, #home_universe textarea, #home_universe select,
	#universe input, #universe textarea, #universe select  {
		font-family:Arial, Helvetica, sans-serif;
		font-size:12px;
		color:#666;
		margin:0px;
		padding:0px;
		border:1px solid #CCC;
		behavior: url(css/iepngfix.htc); 
	}
	
	#home_universe fieldset,
	#universe fieldset {
		margin: 0;
		padding: 0;
		border: none;
	}
	
	#home_universe div#content,
	#universe div#content {
		padding-left: 0px;
	}
	
	
	
	
	
	
