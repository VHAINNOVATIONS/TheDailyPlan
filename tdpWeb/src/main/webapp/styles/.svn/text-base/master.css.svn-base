		display: none;
	}	
	
	#universe #search_results_empty h2 {
		padding: 20px;
	}

	#universe #search_results table {
		margin:0px;
		padding:0px;
		border:none;
		width:100%
	}
	
	#universe #search_results table p {
		margin:0px;
		padding:0px;
		border:none;
	}
	
	#universe #search_results th.head {
		margin:0px;
